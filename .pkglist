{
  "8086tiny": {
    "src": "https://github.com/adriancable/8086tiny.git",
    "sys": "make",
    "dep": "bash, coreutils, ncurses",
    "dsc": "A PC XT-compatible emulator/virtual machine",
    "lic": "MIT",
    "url": "https://github.com/adriancable/8086tiny"
  },
  "libiconv": {
    "src": "https://ftp.gnu.org/pub/gnu/libiconv/libiconv-1.16.tar.gz",
    "dep": "libc++, libicu, libxml2",
    "dsc": "An implementation of iconv()",
    "lic": "LGPL-2.0",
    "url": "https://www.gnu.org/software/libiconv/"
  },
  "libuuid": {
    "src": "https://downloads.sourceforge.net/project/libuuid/libuuid-1.0.3.tar.gz",
    "dep": "libusb",
    "dsc": "Portable uuid C library",
    "lic": "BSD 3-Clause",
    "url": "https://sourceforge.net/projects/libuuid/"
  },
  "libusb": {
    "src": "https://github.com/libusb/libusb/archive/v1.0.24.tar.gz",
    "dep": "libiconv",
    "dsc": "A C library that provides generic access to USB devices",
    "lic": "LGPL-2.1",
    "url": "https://libusb.info/"
  },
  "libx11": {
    "src": "https://gitlab.freedesktop.org/xorg/lib/libx11.git",
    "sys": "ag",
    "dep": "libxcb",
    "dsc": "X11 client-side library",
    "lic": "MIT",
    "url": "https://xorg.freedesktop.org/"
  },
  "libxau": {
    "src": "https://xorg.freedesktop.org/releases/individual/lib/libXau-1.0.9.tar.bz2",
    "dep": "libc++, libuuid, zlib",
    "dsc": "X11 authorisation library",
    "lic": "MIT",
    "url": "https://xorg.freedesktop.org/"
  },
  "libxcb": {
    "src": "https://xorg.freedesktop.org/archive/individual/lib/libxcb-1.14.tar.xz",
    "dep": "libxau, libxdmcp",
    "dsc": "X11 client-side library",
    "lic": "MIT",
    "url": "https://xcb.freedesktop.org/"
  },
  "libxdmcp": {
    "src": "https://xorg.freedesktop.org/archive/individual/lib/libXdmcp-1.1.3.tar.bz2",
    "dep": "libx11, libxfixes, libxrender",
    "dsc": "X11 Display Manager Control Protocol library",
    "lic": "MIT",
    "url": "https://xorg.freedesktop.org/"
  },
  "libxfixes": {
    "src": "https://xorg.freedesktop.org/releases/individual/lib/libXfixes-5.0.3.tar.bz2",
    "dep": "libx11",
    "dsc": "X11 miscellaneous 'fixes' extension library",
    "lic": "MIT",
    "url": "https://xorg.freedesktop.org/"
  },
  "libxrender": {
    "src": "https://xorg.freedesktop.org/releases/individual/lib/libXrender-0.9.10.tar.bz2",
    "dep": "libx11",
    "dsc": "X Rendering Extension client library",
    "lic": "MIT",
    "url": "https://xorg.freedesktop.org/"
  }
}